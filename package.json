{
  "name": "cloud-build-cache",
  "version": "0.1.1",
  "description": "Cache folders such as node_modules in gcs and restore them on next builds to reduce build time.",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bin": {
    "cbc": "./bin/cbc.js"
  },
  "files": [
    "lib",
    "esm",
    "types"
  ],
  "scripts": {
    "build:cjs": "tsc --declarationdir types --tsbuildinfofile .tsbuildinfo.esm",
    "build:es": "tsc -m esNext --outDir esm --declaration false",
    "build": "npm run build:cjs && npm run build:es",
    "lint": "eslint --ext .js,.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf lib esm types",
    "preversion": "npm run build && npm run lint",
    "version": "npm run format && git add -A",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thdk/cloud-build-cache.git"
  },
  "author": "Thomas Dekiere <t.dekiere@gmail.com>(http://github.com/thdk)",
  "license": "ISC",
  "keywords": [
    "cloud build",
    "cache",
    "node_modules",
    "artifacts",
    "restore",
    "cloud storage"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/node": "^14.14.6",
    "@types/tar": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.4.0",
    "commander": "^6.2.0",
    "del": "^6.0.0",
    "package-changed": "^1.2.1",
    "queue-promise": "^2.1.0",
    "tar": "^6.0.5"
  }
}
